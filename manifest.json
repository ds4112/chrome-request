{
	"name": "Chrome Request",
	"manifest_version": 2,
	"description": "none",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["script.js"]
		}
	],
	"permissions": [
		"activeTab","tabs","http://*/*","contextMenus"
	],
	"web_accessible_resources": [
	]
}
